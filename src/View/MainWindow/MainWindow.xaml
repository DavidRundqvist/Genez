<Window x:Class="View.MainWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:view="clr-namespace:View"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"         
        Title="Genez" 
        d:DesignHeight="400" 
        d:DesignWidth="600"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:controls="clr-namespace:GraphX.Controls;assembly=GraphX.Controls"
        xmlns:personList="clr-namespace:View.PersonList"
        xmlns:mainWindow="clr-namespace:View.MainWindow"
        xmlns:ancestryGraph="clr-namespace:View.AncestryGraph"
        mc:Ignorable="d" 
        d:DataContext="{d:DesignInstance mainWindow:DesignMainWindowPresentation, IsDesignTimeCreatable=True}" WindowState="Maximized" WindowStartupLocation="CenterScreen">

    <DockPanel >
        <personList:PersonList DataContext="{Binding PersonListPresentation}" DockPanel.Dock="Left"></personList:PersonList>
        <ancestryGraph:AncestryGraph DataContext="{Binding AncestryGraphPresentation}"></ancestryGraph:AncestryGraph>


    </DockPanel>


</Window>
