<Window x:Class="Launch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:view="clr-namespace:View"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"         
        Title="Genez" Height="350" Width="525"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:designPresentation="clr-namespace:View.DesignPresentation"
        mc:Ignorable="d" 
        d:DataContext="{d:DesignInstance designPresentation:DesignMainWindowPresentation, IsDesignTimeCreatable=True}">

        <DockPanel >
            <view:PersonList DataContext="{Binding PersonListPresentation}" DockPanel.Dock="Left"></view:PersonList>


    </DockPanel>


</Window>
