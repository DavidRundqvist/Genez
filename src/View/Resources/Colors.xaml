<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LightGreen">#D3E9BD</Color>
    <Color x:Key="Green">#B7CF6E</Color>
    <Color x:Key="DarkGreen">#1C3808</Color>

    <Color x:Key="Brown">#AB8965</Color>
    <Color x:Key="DarkBrown">#573B20</Color>

    <Color x:Key="White">#F7F3F7</Color>
    
    <Color x:Key="Male">#3C4C7B</Color>
    <Color x:Key="Female">#C25F6B</Color>
    <Color x:Key="Unknown">#CCC870</Color>

    <SolidColorBrush x:Key="MaleBrush" Color="{StaticResource Male}"></SolidColorBrush>
    <SolidColorBrush x:Key="FemaleBrush" Color="{StaticResource Female}"></SolidColorBrush>
    <SolidColorBrush x:Key="UnknownBrush" Color="{StaticResource Unknown}"></SolidColorBrush>

    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource White}"></SolidColorBrush>
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource DarkGreen}" ></SolidColorBrush>
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource DarkBrown}" ></SolidColorBrush>

    <LinearGradientBrush x:Key="SelectionBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="{StaticResource LightGreen}"></GradientStop>
        <GradientStop Offset="1" Color="{StaticResource Green}"></GradientStop>
    </LinearGradientBrush>

</ResourceDictionary>

    
    