<UserControl x:Class="View.AncestryGraph.RelationDetailsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:global="clr-namespace:View.Global"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             d:DataContext="{d:DesignInstance global:DesignRelationPresentation, IsDesignTimeCreatable=True}">
    <StackPanel>
        <Label Content="{Binding Description.Value}"></Label>
    </StackPanel>
</UserControl>
