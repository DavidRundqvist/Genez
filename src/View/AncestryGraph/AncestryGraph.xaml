<UserControl x:Class="View.AncestryGraph.AncestryGraph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:GraphX.Controls;assembly=GraphX.Controls"
             xmlns:ancestryGraph="clr-namespace:View.AncestryGraph"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="600">    
    <controls:ZoomControl x:Name="_zoomControl">
        <ancestryGraph:AncestryGraphArea x:Name="_graphArea"></ancestryGraph:AncestryGraphArea>
    </controls:ZoomControl>    
</UserControl>
